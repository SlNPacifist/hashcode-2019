<?xml version="1.0" encoding="UTF-8"?>
<solver xmlns="https://www.optaplanner.org/xsd/solver" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://www.optaplanner.org/xsd/solver https://www.optaplanner.org/xsd/solver/solver.xsd">

  <solutionClass>domain.Album</solutionClass>
  <entityClass>domain.Slot</entityClass>

  <scoreDirectorFactory>
    <incrementalScoreCalculatorClass>solver.ScoreCalc</incrementalScoreCalculatorClass>
  </scoreDirectorFactory>

  <termination>
    <unimprovedSecondsSpentLimit>10</unimprovedSecondsSpentLimit>
  </termination>
  <constructionHeuristic>
  </constructionHeuristic>
  <localSearch>
    <moveIteratorFactory>
      <moveIteratorFactoryClass>PlaceSlideMoveFactory</moveIteratorFactoryClass>
<!--      <selectionOrder>ORIGINAL</selectionOrder>-->
    </moveIteratorFactory>
  </localSearch>
</solver>
